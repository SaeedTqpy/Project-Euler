First_num = 1
last_num = 0
temp = 0
def fibo (first_num,last_num):
    Sum = 0
    while(Sum<=4000000):
        temp = first_num + last_num
        print(temp)     # for Testing 
        if(temp % 2 ==0):
          Sum += temp
        first_num = last_num
        last_num = temp
    return Sum
print(fibo(2,3))
